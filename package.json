{
  "name": "custom-components-library",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --config ./build/base.config.ts",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "prettier": "prettier --write  .",
    "build": "vue-tsc --noEmit && vite build --config ./build/base.config.ts",
    "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
    "build:doc": "vue-tsc --noEmit && vite build --config ./build/doc.config.ts",
    "com:create": "node script/comp/create/command.js",
    "com:clean": "node script/comp/clean/command.js"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "vue": "^3.2.37",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-vue": "^9.4.0",
    "fs-extra": "^10.1.0",
    "handlebars": "^4.7.7",
    "inquirer": "~8.2.4",
    "npm-run-all": "^4.1.5",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.7.1",
    "prismjs": "^1.29.0",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.7",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-md": "^0.20.4",
    "vue-eslint-parser": "^9.0.3",
    "vue-router": "^4.1.5",
    "vue-tsc": "^0.39.5"
  }
}
